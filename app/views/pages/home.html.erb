<div>
  <h2>Cats</h2>
  <% @cats.each do |c| %>
  	Name: <%=c.name%>
  	<br>
  <% end %>
  <h2>Users</h2>
  <% @users.each do |u| %>
  	Username: <%=u.username%>
  	<br>
  	Email: <%=u.email%>
  	<br>
  	Age: <%=u.age%>
  	<br>
  <% end %>
  <h2>Todos</h2>
  <% @todos.each do |t| %>
  	Task: <%=t.tasks%>
  	<br> 
  	Finished: <%=t.finished%>
  	<br>
  <% end %>

</div>
